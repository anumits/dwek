# map the direct_first_name field
MAP "direct_first_name" AS {direct} WITH form_name = "names" AND form_field = "first_name";

# map the copied_name field
MAP "copied_name" AS {attribute} WITH attribute = "direct_first_name";

# map the name_included field
MAP "name_included" AS {inclusion} WITH value_list = ["Kevin", "Ezra"] AND attribute = "copied_name";
